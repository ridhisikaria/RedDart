{
  "name": "reddart",
  "version": "1.0.0",
  "description": "Time pass Hackathon project",
  "main": "build/src/app.js",
  "scripts": {
    "build": "rm -rf build && tsc",
    "dev": "ts-node-dev --respawn ./src/app.ts",
    "prod": "npm run-script lint && npm run-script build && npx ./build/src/app.js",
    "test": "mocha --exit -r ts-node/register 'test/**/*.spec.ts' --timeout 15000",
    "lint": "eslint --quiet ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts",
    "coverage": "rm -rf coverage && ./node_modules/.bin/nyc --reporter lcov --reporter html --report-dir coverage npm run test",
    "console": "rm -rf build && tsc && node ./console.js",
    "audit": "./node_modules/.bin/check-audit",
    "set-env": "./set-env.sh",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:drop": "sequelize-cli db:drop",
    "db:seed": "sequelize-cli db:seed:all",
    "docker:start": "node ./build/src/app.js",
    "script:updateAllUsersTokenBalance": "node ./build/src/scripts/updateAllUsersTokenBalance.js",
    "script:getNetworkWallets": "node ./build/src/scripts/getNetworkWallets.js",
    "script:clearInvestedQuantityBalance": "node ./build/src/scripts/clearInvestedQuantityBalance.js",
    "script:deleteUserRecords": "node ./build/src/scripts/deleteUserRecords.js",
    "script:migrateTokenId": "node ./build/src/scripts/migrateTokenIds.js",
    "script:migrateTokenGroupId": "node ./build/src/scripts/migrateTokenGroupIds.js",
    "script:populateTransfers": "node ./build/src/scripts/populateTransfers.js",
    "docker:up": "source .env && docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "author": "Baka People",
  "license": "ISC",
  "dependencies": {
    "@mysten/sui.js": "^0.44.0",
    "@spheron/storage": "^2.0.3",
    "await-to-js": "^3.0.0",
    "axios": "^0.27.2",
    "ci": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "js-big-decimal": "^1.3.5",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-replace-enum-postgres": "^1.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
